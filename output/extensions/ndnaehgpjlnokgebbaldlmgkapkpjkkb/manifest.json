{
  "background": {
    "scripts": [
      "scripts/lib/lodash.min.js",
      "scripts/lib/jquery.min.js",
      "scripts/lib/handlebars.runtime.min.js",
      "templates/modals.tmpl.js",
      "scripts/bundles/background.bundle.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon-grey-32.png",
    "default_popup": "templates/popup.html",
    "default_title": "__MSG_appName__"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/lib/intercom-snippet.js"
      ],
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "scripts/gmail.js",
        "scripts/viewdata.js"
      ],
      "matches": [
        "https://mail.google.com/*"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "scripts/inbox.js"
      ],
      "matches": [
        "https://inbox.google.com/*"
      ],
      "run_at": "document_start"
    },
    {
      "css": [
        "styles/style.css"
      ],
      "js": [
        "scripts/lib/lodash.min.js",
        "scripts/lib/jquery.min.js",
        "scripts/lib/bootstrap.min.js",
        "scripts/lib/Urlencode.js"
      ],
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "run_at": "document_end"
    },
    {
      "js": [
        "scripts/bundles/gmail.end.bundle.js"
      ],
      "matches": [
        "https://mail.google.com/*"
      ],
      "run_at": "document_end"
    },
    {
      "js": [
        "scripts/bundles/inbox.end.bundle.js"
      ],
      "matches": [
        "https://inbox.google.com/*"
      ],
      "run_at": "document_end"
    },
    {
      "js": [
        "scripts/lib/lodash.min.js",
        "scripts/bundles/setup.bundle.js"
      ],
      "matches": [
        "*://mailtrack.io/*/dashboard/welcome*",
        "*://mailtrack.io/*/dashboard/reauthorized*"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "scripts/bundles/dashboard.bundle.js"
      ],
      "matches": [
        "*://mailtrack.io/en/*",
        "*://mailtrack.io/es/*",
        "*://mailtrack.io/pt/*",
        "*://mailtrack.io/ca/*"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "default_locale": "en",
  "description": "__MSG_appDescription__",
  "homepage_url": "https://mailtrack.io",
  "icons": {
    "128": "images/icon-128.png",
    "16": "images/icon-16.png"
  },
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "permissions": [
    "*://*.mailtrack.io/*",
    "*://mailtrack.io/*",
    "https://mail.google.com/*",
    "*://*.googleusercontent.com/*",
    "https://inbox.google.com/*",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "notifications",
    "contextMenus",
    "alarms"
  ],
  "version": "2.32.3",
  "web_accessible_resources": [
    "conf/configuration.json",
    "images/buttons-crx-sprite_1x.png",
    "images/buttons-crx-sprite_2x.png",
    "images/mailtrack-crx-sprite_2x.png",
    "https://mailtrack.io/recipients",
    "templates/iframe.html",
    "templates/iframe.css",
    "templates/iframe.js",
    "scripts/lib/intercom-shim.js",
    "scripts/lib/intercom-frame.js",
    "scripts/lib/rollbar.min.js"
  ]
}